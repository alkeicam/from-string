  "repository": {
    "type": "git",
    "url": "https://github.com/alkeicam/from-string.git"
  },
  "keywords": [
    "string",
    "parse",
    "array",
    "number"
  ],
  "files": [
    "dist",
    "src"
  ],
  "author": "Maciej Grula <maciej.grula@gmail.com>",
  "license": "MIT",
  "unpkg": "dist/from-string.js",
  "dependencies": {},
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "mocha": "^5.0.5",
    "nyc": "14.1.x",
    "sinon": "^4.1.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "1.7.x"
  },
  "nyc": {
    "check-coverage": true,
    "branches": 100,
    "lines": 100,
    "functions": 100,
    "statements": 100
  }
